{
  "name": "zora",
  "version": "1.1.0",
  "description": "CSS compiler for the Zora system",
  "scripts": {
    "build": "sass src/scss/zora.scss css/zora.css --style=expanded --no-source-map",
    "watch": "sass src/scss/zora.scss css/zora.css --style=expanded --source-map --watch",
    "generate": "node src/generate-zora-system.mjs"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Syphre",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "glob": "^10.3.10",
    "inquirer": "^9.2.15",
    "prompt": "^1.3.0",
    "renamer": "^5.0.0",
    "replace": "^1.2.2",
    "sass": "^1.53.0"
  }
}
